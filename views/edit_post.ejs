<% include /layouts/header.ejs %>
     <h2>Izmenite vas post</h2>
     <div class="newpost">
     <form action="/posts/edit/" method="post" target="_blank">
        <input type="hidden" name="id" value="<%= post.id %>">
        Naslov:<br>
        <input type="text" name="title" value="<%= post.title %>"><br>
