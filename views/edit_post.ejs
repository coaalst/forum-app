        <input type="text" name="tweet" size="50" height="200" value="<%= post.tweet %>"><br><br>
        <input type="hidden" name="userid" value="<%= post.userid %>">
        <input type="submit" value="Send">
        <input type="reset" value="Reset">
        </form>
    </div>
